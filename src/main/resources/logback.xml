<configuration>

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>SPARK PAC4J DEMO %d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>

  <!--logger name="org.pac4j" level="DEBUG" /-->
  
  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
